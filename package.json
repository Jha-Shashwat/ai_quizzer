{
  "name": "ai-quizzer-backend",
  "version": "1.0.0",
  "description": "AI-powered Quiz Application Backend with authentication, quiz management, and score tracking",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "setup": "npm run migrate && echo 'Database setup complete!'",
    "build": "echo 'Build completed'",
    "lint": "eslint src/ --ext .js"
  },
  "keywords": [
    "quiz",
    "ai",
    "education",
    "backend",
    "rest-api",
    "jwt",
    "groq"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "groq-sdk": "^0.3.3",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@types/jest": "^29.5.8"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}